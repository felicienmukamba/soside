{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/soside-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/soside-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/soside-backend",
  "projects": {
    "ai-service": {
      "type": "application",
      "root": "apps/ai-service",
      "entryFile": "main",
      "sourceRoot": "apps/ai-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ai-service/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "blog-service": {
      "type": "application",
      "root": "apps/blog-service",
      "entryFile": "main",
      "sourceRoot": "apps/blog-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/blog-service/tsconfig.app.json"
      }
    },
    "community-service": {
      "type": "application",
      "root": "apps/community-service",
      "entryFile": "main",
      "sourceRoot": "apps/community-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/community-service/tsconfig.app.json"
      }
    },
    "learning-service": {
      "type": "application",
      "root": "apps/learning-service",
      "entryFile": "main",
      "sourceRoot": "apps/learning-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/learning-service/tsconfig.app.json"
      }
    },
    "project-service": {
      "type": "application",
      "root": "apps/project-service",
      "entryFile": "main",
      "sourceRoot": "apps/project-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/project-service/tsconfig.app.json"
      }
    },
    "recruitment-service": {
      "type": "application",
      "root": "apps/recruitment-service",
      "entryFile": "main",
      "sourceRoot": "apps/recruitment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/recruitment-service/tsconfig.app.json"
      }
    },
    "soside-backend": {
      "type": "application",
      "root": "apps/soside-backend",
      "entryFile": "main",
      "sourceRoot": "apps/soside-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/soside-backend/tsconfig.app.json"
      }
    }
  }
}